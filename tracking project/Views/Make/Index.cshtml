@model IEnumerable<tracking_project.Models.Make>

@{
    ViewData["Title"] = "Index";
}

<h1>Make Details</h1>

<p>
    <a asp-action="MakeCreate">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MakeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MakeName)
            </td>
            <td>
                <a asp-action="MakeUpdate" asp-route-id="@item.Id">Edit</a> |
               
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
